# AI Horizon RAG Agent - Environment Configuration
# Copy this file to .env and fill in your actual credentials

# ================================
# SUPABASE CONFIGURATION
# ================================
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here
SUPABASE_DB_URL=postgresql://postgres:your-password@db.your-project-id.supabase.co:5432/postgres

# ================================
# OPENAI CONFIGURATION
# ================================
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4o-mini
OPENAI_EMBEDDING_MODEL=text-embedding-ada-002

# ================================
# DUMPLING AI CONFIGURATION
# ================================
DUMPLING_API_KEY=sk_your-dumpling-api-key-here
DUMPLING_API_URL=https://app.dumplingai.com/api/v1

# ================================
# TELEGRAM BOT CONFIGURATION
# ================================
TELEGRAM_BOT_TOKEN=your-telegram-bot-token-here
TELEGRAM_WEBHOOK_URL=https://your-n8n-instance.com/webhook/telegram-bot-horizon

# ================================
# N8N CONFIGURATION
# ================================
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_WEBHOOK_URL=https://your-n8n-instance.com

# ================================
# APPLICATION SETTINGS
# ================================
APP_ENV=development
LOG_LEVEL=info
DEBUG=true

# ================================
# VECTOR SEARCH SETTINGS
# ================================
VECTOR_SIMILARITY_THRESHOLD=0.7
MAX_SEARCH_RESULTS=10
EMBEDDING_DIMENSIONS=1536

# ================================
# DOCUMENT PROCESSING SETTINGS
# ================================
MAX_DOCUMENT_SIZE_MB=30
MAX_VIDEO_SIZE_GB=2
CHUNK_SIZE=1500
CHUNK_OVERLAP=200

# ================================
# RATE LIMITING
# ================================
OPENAI_RPM_LIMIT=3000
DUMPLING_RPM_LIMIT=100
TELEGRAM_MESSAGE_LIMIT=30